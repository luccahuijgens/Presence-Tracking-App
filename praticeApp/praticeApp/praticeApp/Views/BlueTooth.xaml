<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="praticeApp.Views.BlueTooth"
             Title="Registratie">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="statusLabel" Text="Druk op de onderstaande button om de activatiemodus te activeren."/>
            <Button x:Name ="activateBtn" Text="Activeer registratiemodus" Clicked="ActivateBluetooth" BackgroundColor="#00a98c" TextColor="White" />
            <Label x:Name="beaconsText" Margin="5, 5, 5, 5"/>
          
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>